{
  "private": true,
  "name": "remix-jokes-backend",
  "version": "0.0.0",
  "license": "MIT",
  "main": "server.ts",
  "engines": {
    "node": "16.14.0"
  },
  "scripts": {
    "dev": "nodemon --exec \"ts-node --files ./src/server.ts\" -e .ts",
    "build": "prisma migrate deploy && tsc",
    "prestart": "tsc",
    "start": "node dist/server.js",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "lint": "TIMING=1 eslint src/'**/*.{ts,tsx}' --fix",
    "format:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
    "format:fix": "prettier --write .",
    "clean": "rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc -b",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "redis": "^4.0.3",
    "typescript": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.14",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "2.6.0",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.10.0",
    "ts-node": "^10.4.0"
  }
}
